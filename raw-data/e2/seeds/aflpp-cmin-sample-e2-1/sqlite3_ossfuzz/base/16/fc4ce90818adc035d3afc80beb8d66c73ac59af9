 CREATE TABLE t3928(a INTEGPRIMARY KEY AUTOINCRDMENT, b);	      CREATE TRIGGER t3928R INSERT ON t3928 BEGIN
   DELETE FROM t1;
    INSERT INTO t1(a,b,c,d,e) VALUES(1,2,3,4,5);
    INSERT INTO t1(a,b,c,d,e) VALUES(1,NULL,3,4,5)
      ON CONFLICT(c) DO UPDATE SET b='c'
       typeof(1)T(a) DO UPDATE SET b='a'
      ON CONFLICT(d) DO UPDATE SET b='d'   INSERT INTO t1(a,b,c